<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:wimm="http://schemas.wimm.com/android"
    package="jp.kisaka.toshihiro.android.wimm.tally"
    android:versionCode="1"
    android:versionName="1.0.0" >

    <uses-sdk android:minSdkVersion="7" />

    <application
        android:icon="@drawable/tally"
        android:label="@string/app_name" >
        <uses-library
            wimm:minSdkVersion="1.0.2"
            android:name="com.wimm.framework" />

        <activity
            android:label="@string/app_name"
            android:name=".TallyActivity" >
            <meta-data
                android:name="com.wimm.app.peekview"
                android:resource="@layout/peekview" />

            <intent-filter >
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
